{
    "name": "agelxnash/laravel-magic-dto",
    "keywords": [
        "dto",
        "array",
        "laravel",
        "illuminate"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "agel_nash",
            "email": "laravel@agel-nash.ru",
            "homepage": "https://agel-nash.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9|^10|^11",
        "illuminate/container": "^9|^10|^11"
    },
    "require-dev": {
        "ext-pdo": "*",
        "ext-pdo_sqlite": "*",
        "phpunit/phpunit": "^8.5.30|^9.0",
        "orchestra/testbench-dusk": "^5|^6|^7|^8|^9"
    },
    "suggest": {
        "illuminate/database": "If you want autowiring Eloquent models via \\AgelxNash\\MagicDto\\Attributes\\InjectModel"
    },
    "autoload": {
        "psr-4": {
            "AgelxNash\\MagicDto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AgelxNash\\MagicDto\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "AgelxNash\\MagicDto\\ServiceProvider"
            ]
        }
    }
}
